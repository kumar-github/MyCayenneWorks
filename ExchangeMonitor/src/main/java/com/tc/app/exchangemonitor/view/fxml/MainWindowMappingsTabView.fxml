	<left>
	<VBox spacing="2.0" BorderPane.alignment="CENTER">
	<BorderPane.margin><Insets bottom="5.0" left="2.0" right="0.0" top="5.0" /></BorderPane.margin>
	<padding><Insets bottom="2.0" left="2.0" right="2.0" top="1.0" /></padding>
	<children>
		<TitledPane expanded="false" mnemonicParsing="true" text="_Exchanges" styleClass="titledPaneStyle">
		<content>
			<VBox prefHeight="260.0" prefWidth="150.0" styleClass="normalButtonStyle">
			<children>
				<CheckListView fx:id="externalTradeSourcesListView" focusTraversable="false" prefHeight="100.0" prefWidth="100.0" VBox.vgrow="ALWAYS" />
			</children>
			<padding><Insets top="1.0" /></padding>
			</VBox>
		</content>
		<padding><Insets bottom="1.0" left="1.0" right="1.0" top="1.0" /></padding>
		</TitledPane>
		
		<!--<TitledPane expanded="false" layoutX="12.0" layoutY="11.0" text="Mapping Category">-->
		<TitledPane expanded="false" text="Mapping Category" styleClass="titledPaneStyle">
		<content>
			<VBox spacing="5.0" styleClass="normalButtonStyle">
			<children>
				<Button fx:id="tradersMappingButton" maxWidth="1.7976931348623157E308" mnemonicParsing="true" text="_Traders"  onAction="#handleTradersMappingButtonClick" disable="true"/>
				<Button fx:id="brokersMappingButton" maxWidth="1.7976931348623157E308" mnemonicParsing="true" text="_Brokers" onAction="#handleBrokersMappingButtonClick" disable="true"/>
				<Button fx:id="companiesMappingButton" maxWidth="1.7976931348623157E308" mnemonicParsing="true" text="_Companies" onAction="#handleCompaniesMappingButtonClick" disable="true"/>
				<Button fx:id="currenciesMappingButton" maxWidth="1.7976931348623157E308" mnemonicParsing="true" text="C_urrencies" onAction="#handleCurrenciesMappingButtonClick" disable="true"/>
				<Button fx:id="portfoliosMappingButton" maxWidth="1.7976931348623157E308" mnemonicParsing="true" text="_Portfolios" onAction="#handlePortfoliosMappingButtonClick" disable="true"/>
				<Button fx:id="templateTradesMappingButton" maxWidth="1.7976931348623157E308" mnemonicParsing="true" text="T_emplate Trades" onAction="#handleTemplateTradesMappingButtonClick" disable="true"/>
				<Button fx:id="accountsMappingButton" maxWidth="1.7976931348623157E308" mnemonicParsing="true" text="_Accounts" onAction="#handleAccountsMappingButtonClick" disable="true"/>
				<Button fx:id="uomConversionsMappingButton" maxWidth="1.7976931348623157E308" mnemonicParsing="true" text="U_OM Conversions" onAction="#handleUOMConversionsMappingButtonClick" disable="true"/>
				<Button fx:id="tradingPeriodsMappingButton" maxWidth="1.7976931348623157E308" mnemonicParsing="true" text="T_rading Periods" onAction="#handleTradingPeriodsMappingButtonClick" disable="true"/>
			</children>
			<padding><Insets bottom="5.0" left="5.0" right="5.0" top="5.0" /></padding>
			</VBox>
		</content>
		<padding><Insets bottom="1.0" left="1.0" right="1.0" top="1.0" /></padding>
		</TitledPane>
		
