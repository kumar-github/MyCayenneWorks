<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Button?>

<?import javafx.scene.control.cell.PropertyValueFactory?>

<VBox spacing="10.0"  id="portfoliosMappingVBox" fillWidth="false" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tc.app.exchangemonitor.controller.ExternalMappingPortfoliosController">
<BorderPane.margin><Insets bottom="5.0" left="0.0" right="5.0" top="5.0" /></BorderPane.margin>
<padding><Insets bottom="2.0" left="2.0" right="2.0" top="2.0" /></padding>
<children>
	<HBox spacing="10.0">
	<children>
		<Button text="_Add" mnemonicParsing="true" fx:id="addMappingButton" onAction="#handleAddMappingButtonClick"/>
		<Button disable="true" text="_Delete" mnemonicParsing="true" fx:id="deleteMappingButton" onAction="#handleDeleteMappingButtonClick"/>
		<Button disable="true" text="_Update" mnemonicParsing="true" fx:id="updateMappingButton" onAction="#handleUpdateMappingButtonClick"/>
		<Button text="_Refresh" mnemonicParsing="true" fx:id="refreshMappingButton" onAction="#handleRefreshMappingButtonClick"/>
	</children>
	</HBox>
	<TableView fx:id="externalMappingPortfoliosTableView" cache="true" prefWidth="1250.0" VBox.vgrow="ALWAYS" focusTraversable="false">
	<columns>
		<TableColumn text="Portfolios" styleClass="tableTitle">
		<columns>
			<TableColumn fx:id="externalSourceCommodityTableColumn" minWidth="300.0" text="External Source Commodity">
			<cellValueFactory><PropertyValueFactory property="externalValue1" /></cellValueFactory>
			</TableColumn>
			<TableColumn fx:id="externalSourceTraderTableColumn" minWidth="150.0" text="External Source Trader">
			<cellValueFactory><PropertyValueFactory property="externalValue2" /></cellValueFactory>
			</TableColumn>
			<TableColumn fx:id="externalSourceTradingPeriodTableColumn" minWidth="200.0" text="External Source Trading Period">
			<cellValueFactory><PropertyValueFactory property="externalValue3" /></cellValueFactory>
			</TableColumn>
			<TableColumn fx:id="externalSourceAccountTableColumn" minWidth="150.0" text="External Source Account">
			<cellValueFactory><PropertyValueFactory property="externalValue4" /></cellValueFactory>
			</TableColumn>
			<TableColumn fx:id="ictsPortfolioTableColumn" minWidth="300.0" text="ICTS Portfolio">
			<!--  <cellValueFactory><PropertyValueFactory property="aliasValue" /></cellValueFactory> -->
			<cellValueFactory><PropertyValueFactory property="aliasValueForPortfolios" /></cellValueFactory>
			</TableColumn>
		</columns>
		</TableColumn>
	</columns>
	<columnResizePolicy><TableView fx:constant="CONSTRAINED_RESIZE_POLICY" /></columnResizePolicy>
	</TableView>
</children>
</VBox>